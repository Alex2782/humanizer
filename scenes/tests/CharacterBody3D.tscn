# Called when the node enters the scene tree for the first time.
func _ready():
	var config = HumanConfig.new()
	config.init_macros()
	config.targets['gender'] = 0
	config.targets['age'] = 25
	config.rig = HumanizerGlobalConfig.config.default_skeleton
	var pants = HumanizerEquipment.new(\"SkinnyJeans\")
	pants.texture_name = pants.get_type().textures.keys()[0]
	config.add_equipment(pants)
	var shirt = HumanizerEquipment.new(\"MakeHumanTShirt\")
	shirt.texture_name = shirt.get_type().textures.keys()[0]
	config.add_equipment(shirt)
	var humanizer = Humanizer.new(config)
	humanizer.set_skin_texture(\"young_caucasian_female2\")
	var mi = MeshInstance3D.new()
	mi.mesh = humanizer.standard_bake_meshes()
	add_child(mi)
	
