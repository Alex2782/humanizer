# Called when the node enters the scene tree for the first time.
func _ready():
	var config = HumanConfig.new()
	config.init_macros()
	config.targets['gender'] = 0
	config.targets['age'] = 25
	config.rig = HumanizerGlobalConfig.config.default_skeleton
	var pants = HumanizerRegistry.clothes.SkinnyJeans
	pants.texture_name = pants.textures.keys()[0]
	config.add_equipment(pants)
	var shirt = HumanizerRegistry.clothes.MakeHumanTShirt
	shirt.texture_name = shirt.textures.keys()[0]
	config.add_equipment(shirt)
	var humanizer = Humanizer.new(config)
	humanizer.set_skin_texture(\"young_caucasian_female2\")
	var mi = MeshInstance3D.new()
	mi.mesh = humanizer.get_body_mesh()
	add_child(mi)
	for equip in config.equipment.values():
		var equip_mi = MeshInstance3D.new()
		equip_mi.mesh = humanizer.get_mesh(equip.resource_name)
		add_child(equip_mi)

